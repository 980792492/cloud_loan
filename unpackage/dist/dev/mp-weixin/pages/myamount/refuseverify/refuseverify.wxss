
.container {
	box-sizing: border-box;
	width: 100%;
	height: 100%;
	padding: 30rpx;
	background: url(/static/assets/Group12.png);
	background-repeat: no-repeat;
	background-size: contain;
}
.apply-credit-wrap {
	padding: 20px;
	background: #fff;
	border-radius: 20rpx;
}
.title {
	text-align: center;
	font-size: 32rpx;
	color: #333 ;
	line-height: 96rpx;
}
.tips {
	text-align: center;
	font-size: 28rpx;
	color: #999999 ;
}
.picture-wrap {
	text-align: center;
	margin: 60rpx 0 140rpx;
}
.picture {
	width: 400rpx;
	height: 380rpx;
}





